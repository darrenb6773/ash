    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Launch Long Info",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["info", "CVE-2010-3333"]
        },
        {
            "name": "Launch Calculate High Severity",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["calculate", "--identifiers", "CVE-2010-3333,CVE-2018-11776,CVE-2017-9791,CVE-2018-5407"]
        },
        {
            "name": "Launch Calculate Low Severity",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["calculate", "--identifiers", "CVE-2018-5407"]
        },
    ]
}